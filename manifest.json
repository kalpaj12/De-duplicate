{
    "manifest_version": 2,
    "name": "De-duplicate",
    "version": "2.0.0",
    "author": "Kalpaj Agrawalla",
    "description": "Auto-close duplicate tabs",
    "minimum_chrome_version": "70",
    "icons": {
        "16": "src/icons/16.ico",
        "32": "src/icons/32.ico",
        "48": "src/icons/48.ico",
        "64": "src/icons/64.ico",
        "128": "src/icons/128.ico"
    },
    "browser_action": {
        "default_title": "Click to access settings of this extension.",
        "default_icon": {
            "16": "src/icons/16.ico",
            "32": "src/icons/32.ico",
            "48": "src/icons/48.ico",
            "64": "src/icons/64.ico",
            "128": "src/icons/128.ico"
        },
        "default_popup": "src/html/popup.html"
    },
    "background": {
        "scripts": [
            "src/scripts/background.js"
        ],
        "persistent": true
    },
    "options_ui": {
        "page": "src/html/options.html",
        "open_in_tab": true
    },
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage",
        "contextMenus"
    ]
}