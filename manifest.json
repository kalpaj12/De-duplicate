{
    "manifest_version": 2,
    "name": "De-duplicate",
    "version": "0.0.1",
    "author": "Kalpaj Agrawalla",
    "description": "Close duplicate tabs",
    "browser_action": {
        "default_title": "De-duplicate"
    },
    "background": {
        "scripts": [
            "src/scripts/background.js"
        ],
        "persistent": true
    },
    "content_scripts": [{
        "run_at": "document_idle",
        "all_frames": false,
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "src/scripts/content.js"
        ]
    }],
    "minimum_chrome_version": "75",
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage",
        "contextMenus",
        "contentSettings"
    ]
}